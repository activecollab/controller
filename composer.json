{
    "name": "activecollab/controller",
    "description": "Invokeable controller that accepts containers, works with ETag and more",
    "keywords": [
        "controller"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "activecollab/containeraccess": "^2.0",
        "activecollab/etag": "^1.0",
        "activecollab/templateengine": "^1.0",
        "activecollab/utils": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "~1.0",
        "zendframework/zend-diactoros": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "monolog/monolog": "^1.0",
        "pimple/pimple": "^3.0",
        "phpunit/phpunit": "^5.0",
        "slim/http": "0.1.0"
    },
    "suggest": {
        "slim/http": "Allow using of chunked file download"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Controller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\Controller\\Test\\": "test/src"
        }
    }
}
